<form method="post" action="{{ path('static_data_delete', {'type': static_datum.type}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ static_datum.type) }}">
    <button class="btn">Delete</button>
</form>

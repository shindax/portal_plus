{% extends 'base.html.twig' %}

{% block title %}Weather{% endblock %}

{% block body %}
    <h1>Weather</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ weather.id }}</td>
            </tr>
            <tr>
                <th>Temperature</th>
                <td>{{ weather.temperature }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ weather.name }}</td>
            </tr>
            <tr>
                <th>Pressure</th>
                <td>{{ weather.pressure }}</td>
            </tr>
            <tr>
                <th>Humidity</th>
                <td>{{ weather.humidity }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ weather.type }}</td>
            </tr>
            <tr>
                <th>Winddirection</th>
                <td>{{ weather.winddirection }}</td>
            </tr>
            <tr>
                <th>Windpower</th>
                <td>{{ weather.windpower }}</td>
            </tr>
            <tr>
                <th>Source</th>
                <td>{{ weather.source }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('weather_index') }}">back to list</a>

    <a href="{{ path('weather_edit', {'id': weather.id}) }}">edit</a>

    {{ include('@Informer/weather/_delete_form.html.twig') }}
{% endblock %}
